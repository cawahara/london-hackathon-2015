      <%= form_tag(:controller => :invite, :action => :invite) do %>
      <table class="table">
        <thead>
          <tr>
            <th>Check</th>
            <th>Date</th>
            <th>Events</th>
          </tr>
        </thead>
        <tbody>
          <% @events.each do |date, day_events| %>
          <tr>

            <td>[]</td>
            <td><%= date.strftime('%b %d') %></td>
            <td>
              <% day_events.each do |event| %>
                  <!-- <span class="glyphicon glyphicon-user"></span> -->
                  <%= event.from.strftime('%H:%M') %>-<%= event.to.strftime('%H:%M') %> <%=event.name %> <br />
              <% end %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
      <% end %>

